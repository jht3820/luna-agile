	<build>
		<defaultGoal>install</defaultGoal>
		<directory>${basedir}/target</directory>
		<!-- <finalName>${artifactId}-${version}</finalName> -->
		<finalName>${artifactId}</finalName>
		<pluginManagement>
			<plugins>
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-compiler-plugin</artifactId>
					<configuration>
						<source>1.8</source>
						<target>1.8</target>
						<encoding>UTF-8</encoding>
					</configuration>
				</plugin>
				<plugin>
					<artifactId>maven-war-plugin</artifactId>
					<version>2.3</version>
					<configuration>
						<packagingExcludes>
							WEB-INF/classes/kr/opensoftlab/sdf/commons/dbcp/*.class
							,WEB-INF/classes/kr/opensoftlab/sdf/excel/*.class
							,WEB-INF/classes/kr/opensoftlab/sdf/excel/annotation/*.class
							,WEB-INF/classes/kr/opensoftlab/sdf/excel/file/*.class
							,WEB-INF/classes/kr/opensoftlab/sdf/excel/vo/*.class
							,WEB-INF/classes/kr/opensoftlab/sdf/interceptor/*.class
							,WEB-INF/classes/kr/opensoftlab/sdf/jenkins/service/*.class
							,WEB-INF/classes/kr/opensoftlab/sdf/jenkins/service/impl/*.class
							,WEB-INF/classes/kr/opensoftlab/sdf/jenkins/task/*.class
							,WEB-INF/classes/kr/opensoftlab/sdf/jenkins/vo/*.class
							,WEB-INF/classes/kr/opensoftlab/sdf/paging/*.class
							,WEB-INF/classes/kr/opensoftlab/sdf/svn/*.class
							,WEB-INF/classes/kr/opensoftlab/sdf/svn/handler/*.class
							,WEB-INF/classes/kr/opensoftlab/sdf/svn/vo/*.class
							,WEB-INF/classes/kr/opensoftlab/sdf/util/*.class
							,WEB-INF/classes/kr/opensoftlab/sdf/ux//*.class
							,WEB-INF/classes/kr/opensoftlab/sdf/ux/service/*.class
							,WEB-INF/classes/kr/opensoftlab/sdf/ux/service/impl/*.class
							,WEB-INF/classes/kr/opensoftlab/sdf/ux/web/*.class
						</packagingExcludes>
					</configuration>
				</plugin>
			</plugins>
		</pluginManagement>
